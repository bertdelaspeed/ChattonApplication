{
  "expo": {
    "name": "Chat ON",
    "slug": "chat-on",
    "scheme": "com.resusoft.chattons",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LetsChat_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LetsChat_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.resusoft.chattons",
      "adaptiveIcon": {
        "foregroundImage": "./assets/LetsChat_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c7288063-ce1d-4a4f-9302-cd72519d7f8c"
      }
    },
    "owner": "bertstockage"
  }
}
